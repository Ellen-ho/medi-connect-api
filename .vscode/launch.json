{
  "version": "0.2.0",
  "configurations": [
    {
      "type": "node",
      "request": "attach",
      "name": "Node: Nodemon",
      "processId": "${command:PickProcess}",
      "restart": true,
      "outFiles": ["${workspaceRoot}/build/**/*.js"],
      "sourceMaps": true,
      "port": 9229,
      "trace": true
    }
  ]
}
